SELECT 
	n_u_clc.CR_C, 
	n_u_clc.CRSI,
    n_u_clk.CIAN, 
    n_u_clc.CIDE,
    n_u_clc.CCOL,
	 n_u_clc.CETA 
FROM 
    n_u_clc 
JOIN 
    n_u_clk ON n_u_clc.CR_C = n_u_clk.CR_C 
WHERE 
    n_u_clc.CR_C IN (
        '10400023', '10700016', '11100055', '11700041', '12000135',
        '12900001', '13300021', '13900111', '15400004', '16000068',
        '16500030', '16700038', '16800012', '18100176', '18400005',
        '18500010', '18700013', '18800032', '19800001', '21000013',
        '23100024', '23200024', '23300030', '23400041', '27400071',
        '30700127', '31900070', '32900006', '33200027', '33700015',
        '36000018', '36200050', '36300001', '36400009', '36700016',
        '37400025', '41700023', '41900053', '42100011', '42300064',
        '42900013', '46000002', '46200010', '46300001', '46400020',
        '46500042', '46600060', '46700008', '46900025', '47000005',
        '47300014', '45000462', '42900856', '46500966', '12900821',
        '12900822', '30600979', '19800277', '36710397', '19700438',
        '30800492'
    ) 
ORDER BY 
    n_u_clc.CR_C DESC;
